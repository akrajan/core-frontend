<li class="accordion-group">
  <div class="header">
    <div class="title"><%= service_sku.title %></div>
    <% sku_css_id = "service-#{category_count}-#{service_sku_counter}" %>
    <% sku_target = "#"+"service-#{category_count}-#{service_sku_counter}" %>
    <% category_css_target = "##{category_css_id}" %>
    <a class="btn plus-icon transparent" data-toggle="collapse" data-target=<%= sku_target %> data-parent=<%= category_css_target %>>+</a>
  </div>
  <div id=<%= sku_css_id %> class="collapse">
    <dl class="dl-horizontal">
      <dt>Cost Range:</dt>
      <dd><%= sku_price_range(service_sku) %></dd>
      <dt>Time needed:</dt>
      <dd><%= sku_time(service_sku) %></dd>
      <dt>Next available:</dt>
      <dd>Tue 8/16 at 1:00 pm</dd>
    </dl>
    <button type="button" class="btn btn-primary" data-loading-text="Scheduling service..." data-service-id=<%= service_sku.id %> autocomplete="off">Schedule</button>
  </div>
</li>

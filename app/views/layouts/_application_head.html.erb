<head>
  <title><%= content_for?(:title) ? yield(:title) : "Seattle Home Pro" %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Seattle Home Pro" %>">
  <!-- Bootstrap -->
  <link href="bootstrap-2.3.2/bootstrap/css/bootstrap.css" rel="stylesheet" media="screen">
  <!-- Responsive Design Support --->
  <link href="bootstrap-2.3.2/bootstrap/css/bootstrap-responsive.css" rel="stylesheet" media="screen">
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
  <!-- MISSING!  HTML5 shim, for IE6-8 support of HTML5 elements -->
</head>
